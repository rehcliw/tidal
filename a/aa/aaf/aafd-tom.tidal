  , id
  $ note (scale "lydian" 
  $ echoWith 2 0 ((fast 0.5) . (|+ 7)) 
  $ ("<0.11 0.33 0.555 0.777>" ~>)
  $ pat2 + "<0 7 14>" + "-3")
  # s "vscovibes:0"
] 
